<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background" android:gravity="center_horizontal">

    <TextView
        android:id="@+id/introText"
        android:layout_marginTop="20dp"
        android:background="#1A4D21"
        android:padding="10dp"
        android:text="@string/introText" android:layout_height="90dp" android:layout_width="300dp"/>

<LinearLayout
    android:id="@+id/linearLayout1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center" >

</LinearLayout>
<ImageButton android:src="@drawable/icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/imageButton2" android:onClick="startEntryPlayers" android:layout_marginTop="30dp"></ImageButton>

</LinearLayout>
